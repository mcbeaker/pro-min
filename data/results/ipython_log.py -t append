# IPython log file

get_ipython().run_line_magic('logon', '')
get_ipython().run_line_magic('logstart', 'ipython_log.py append')
get_ipython().run_line_magic('logstart', 'ipython_log.py append -t')
get_ipython().run_line_magic('logoff', '')
get_ipython().run_line_magic('logstop', '')
get_ipython().run_line_magic('lsmagic', '')
get_ipython().run_line_magic('logstart', 'ipython_log.py -t append')
get_ipython().run_line_magic('logstop', '')
